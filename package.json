{
  "name": "tslog",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/nodejs/index.js",
  "browser": {
    "tslog": "./dist/browser/index.js",
    "util": "",
    "./src/runtime/nodejs.ts": "./src/runtime/browser.ts"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.src.json tsconfig.example.json && node build.js",
    "start": "node example/index.js",
    "dev-ts": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --no-warnings --loader ts-node/esm' example/nodejs/index.ts",
    "dev-js": "tsc -b tsconfig.example.json && node --enable-source-maps --experimental-specifier-resolution=node example/dist/example/nodejs/index.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "typedoc": "typedoc"
  },
  "keywords": [],
  "author": "Eugene <opensource@terehov.de> (http://fullstack.build/)",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "clearMocks": true,
    "testMatch": [
      "**/tests/*.test.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^17.0.29",
    "esbuild": "^0.15.5",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.11",
    "typescript": "^4.7.4"
  },
  "dependencies": {}
}
